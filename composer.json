{
    "name": "gracerpro/convert-sport-activity",
    "type": "library",
    "description": "This library can to read ZIP-archive of sport activities. Support two services, Strava and Adidas running.",
    "authors": [
        {
            "name": "Vyacheslav",
            "email": "gracerpro@yandex.ru"
        }
    ],
    "require": {
        "php": "~8.3",
        "ext-json": "*",
        "ext-zip": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.73",
        "phpunit/phpunit": "^12.0"
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Gracerpro\\ConvertSportActivity\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gracerpro\\ConvertSportActivity\\Tests\\": "tests/"
        }
    }
}
